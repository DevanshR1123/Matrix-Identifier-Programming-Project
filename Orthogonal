#include <stdio.h>
void orthogonal(int x[10][10],int a,int b) 
{
    int transx[10][10],multiplcation[10][10],i,j,count=0;
    for(i=0;i<a;i++)
    {
        for(j=0;j<b;j++)
        {
           transx[i][j]=x[i][j];
        }
    }
    transpose(transx[10][10],a,b);
    multiply(multiplication[10][10],x[10][10],transx[10][10],a,a,b);
    for(i=0;i<b;i++)
    {
        if(multiplcation[i][i]==1)
            count++;
    }
    if(count==b)
        printf("Matrix is Orthogonal");
    else
        printf("Matrix is not Orthogonal");
}
